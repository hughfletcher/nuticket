{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.0.*",
		"adldap/adLDAP": "dev-master#ee471d2243124dcf29efd8c079456cc56e2fa6e7",
		"caffeinated/menus": "~1.0",
		"caffeinated/modules": "~1.0",
		"bosnadev/repositories": "dev-master#637c703",
		"wikimedia/composer-merge-plugin": "1.2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1",
    	"itsgoingd/clockwork": "~1.7",
    	"codeception/codeception": "*",
    	"mockery/mockery": "dev-master",
    	"codeception/codeception": "*",
    	"mockery/mockery": "dev-master",
    	"fzaninotto/faker": "1.5.0"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/",
			"Modules\\": "modules/",
			"adLDAP\\": "vendor/adldap/adldap/lib"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"extra": {
	    "merge-plugin": {
	        "include": [
	            "extensions/*/composer.json"
	        ]
	    }
	}
}
